{
	"manifest_version": 2,
	
	"name": "Gist It",
	"version": "2.1",
	"description": "Lets you easily embed files from a github repository like a gist.",
	
	"icons": { 
		"16": "img/github16.png",
		"32": "img/github32.png",
		"48": "img/github48.png",
		"128": "img/github128.png"
		
	},
	"background": {
		"scripts": ["embed.js"],
		"permissions": false
	},
	
	"homepage_url": "https://github.com/ishu3101/gist-it",
	
	"permissions": [ 
		"contextMenus"
	]
}
